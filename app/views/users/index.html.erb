<div class="panel panel-primary">
  <div class="panel-heading text-center">
	<span class="h1">Users</span>
  </div>

  <table class="table table-striped table-hover">
	<thead>
	  <tr>
		<th>Name</th>
		<th>Email</th>
		<th>Experience</th>
	  </tr>
	</thead>
	<tbody>
	  <% @users.each do |user| %>
	  <tr data-link="<%= user_path(user) %>">
		<td><%= user.name %></td>
		<td><%= user.email %></td>
		<td><%= user.exp %></td>
	  </tr>
	  <% end %>
	</tbody>
  </table>
</div>